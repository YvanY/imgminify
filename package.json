{
  "name": "imgminify",
  "version": "1.0.0",
  "description": "An online image optimizer",
  "repository": {
    "type": "git",
    "url": "git@github.com:wallstreetcn/imgminify.git"
  },
  "homepage": "https://github.com/wallstreetcn/imgminify#readme",
  "scripts": {
    "start": "cross-env NODE_ENV=production server/bin/www",
    "dev": "cross-env NODE_ENV=development npx nodemon server/bin/www",
    "clean:build": "rimraf server/public/index.html && rimraf server/public/assets",
    "clean:images": "rimraf server/public/images/*",
    "build": "npm run clean:build && cross-env NODE_ENV=production webpack --config build/webpack.conf.js",
    "lint": "eslint --ext .js,.vue client server"
  },
  "author": "Yvan Yang",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.9.2",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
